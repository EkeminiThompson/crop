{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "build": {
    "env": {
      "PYTHONUNBUFFERED": "1"
    },
    "command": "apt-get update && apt-get install -y python3-distutils && pip install -r requirements.txt"
  }
}
